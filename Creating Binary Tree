#include <bits/stdc++.h>
using namespace std;

struct Node {
  int data;
  Node *left, *right;
  Node(int val) {
    data = val;
    left = nullptr;
    right = nullptr;
  }
};

int main() {
  Node *root = new Node(5);
  root->left = new Node(6);
  root->right = new Node(8);
  root->left->left = new Node(10);
  root->left->right = new Node(11);
  root->right->left = new Node(4);
  root->right->right = new Node(3);
}
